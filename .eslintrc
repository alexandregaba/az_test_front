{
  "extends": ["react-app","eslint:recommended"],

  "plugins": [
    "prettier"
  ],

  "rules": {
    "prettier/prettier": ["error", {
      "printWidth": 100,
      "singleQuote": true,
      "trailingComma": "all",
      "bracketSpacing": true,
      "jsxBracketSameLine": true,
      "parser": "flow"
    }]
  }
}
